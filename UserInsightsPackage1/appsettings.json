{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "InstrumentationKey": "YOUR_INSTRUMENTATION_KEY"
  },
  "UserTracking": {
    "Enabled": true,
    "UserAgentTracking": true,
    "IpAddressTracking": true,
    "PageAccessTracking": true
    // Add more configuration settings as needed
  }
}
